{"ast":null,"code":"import AddNote from \"./components/AddNote.vue\";\nimport HeaderComponent from \"./components/HeaderComponent.vue\";\nimport NotesContainer from \"./components/NotesContainer.vue\";\nimport TimerComponent from \"./components/TimerComponent.vue\";\nexport default {\n  name: 'App',\n  components: {\n    AddNote,\n    HeaderComponent,\n    NotesContainer,\n    TimerComponent\n  },\n  methods: {\n    createNote(contents) {\n      if (contents.trim() == \"\") {\n        return;\n      }\n      this.$refs[\"notes-container\"].addNote(this.$refs[\"timer-component\"].getTime(), this.sanitise(contents));\n      this.$refs[\"timer-component\"].startTimer();\n    },\n    copyNotes() {\n      this.$refs[\"notes-container\"].copyToClipboard();\n    },\n    downloadNotes() {\n      this.$refs[\"notes-container\"].downloadNotes();\n    },\n    sanitise(text) {\n      let words = text.split(\" \");\n      // TODO: Find a way to distinguish between her (possessive) and her (referential).\n      for (let i = 0; i < words.length; i++) {\n        let word = words[i].toLowerCase();\n        if (word == \"he\" || word == \"him\" || word == \"she\" || word == \"her\" || word == \"they\") {\n          words[i] = \"TC\";\n        } else if (word == \"his\" || word == \"their\") {\n          words[i] = \"TC's\";\n        }\n      }\n      return words.join(\" \");\n    }\n  }\n};","map":{"version":3,"mappings":"AAUA,OAAOA,OAAM,MAAO,0BAAyB;AAC7C,OAAOC,eAAc,MAAO,kCAAiC;AAC7D,OAAOC,cAAa,MAAO,iCAAgC;AAC3D,OAAOC,cAAa,MAAO,iCAAgC;AAE3D,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVL,OAAO;IACPC,eAAe;IACfC,cAAc;IACdC;EACF,CAAC;EACDG,OAAO,EAAE;IACPC,UAAU,CAACC,QAAQ,EAAE;MACnB,IAAIA,QAAQ,CAACC,IAAI,EAAC,IAAK,EAAE,EAAE;QACzB;MACF;MACA,IAAI,CAACC,KAAK,CAAC,iBAAiB,CAAC,CAACC,OAAO,CAAC,IAAI,CAACD,KAAK,CAAC,iBAAiB,CAAC,CAACE,OAAO,EAAE,EAAE,IAAI,CAACC,QAAQ,CAACL,QAAQ,CAAC,CAAC;MACvG,IAAI,CAACE,KAAK,CAAC,iBAAiB,CAAC,CAACI,UAAU,EAAE;IAC5C,CAAC;IACDC,SAAS,GAAG;MACV,IAAI,CAACL,KAAK,CAAC,iBAA